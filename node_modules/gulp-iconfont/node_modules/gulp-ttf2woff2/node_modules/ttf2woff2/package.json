{
  "name": "ttf2woff2",
  "version": "1.2.3",
  "description": "Convert TTF files to WOFF2 ones.",
  "main": "src/index.js",
  "browser": "jssrc/index.js",
  "engines": {
    "node": ">=0.12.0 <1"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "mocha tests/*.mocha.js",
    "coveralls": "istanbul cover _mocha --report lcovonly -- tests/*.mocha.js -R spec -t 5000 && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cover": "istanbul cover --report html _mocha -- tests/*.mocha.js -R spec -t 5000",
    "cli": "env NPM_RUN_CLI=1",
    "configure": "node-gyp configure",
    "lint": "eslint src/*.js tests/*.src jssrc/index.js",
    "make": "node-gyp build",
    "emcc": "miniquery -p \"targets.#.sources.#\" ./binding.gyp | grep -v \"csrc/addon.cc\" | xargs emcc -o jssrc/ttf2woff2.js -O3 --memory-init-file 0  -s EXPORTED_FUNCTIONS=\"['_convertTTFToWOFF2','_freeTTFToWOFF2']\" -s \"TOTAL_MEMORY=536870912\" -s \"ALLOW_MEMORY_GROWTH=1\" --post-js jssrc/post.js csrc/fallback.cc",
    "emcc-debug": "miniquery -p \"targets.#.sources.#\" ./binding.gyp | grep -v \"csrc/addon.cc\" | xargs emcc -o jssrc/ttf2woff2.js -s EXPORTED_FUNCTIONS=\"['_convertTTFToWOFF2','_freeTTFToWOFF2']\" -s \"ALLOW_MEMORY_GROWTH=1\" -s \"ASSERTIONS=1\" --post-js jssrc/post.js csrc/fallback.cc",
    "install": "(node-gyp rebuild > builderror.log) || (exit 0)"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nfroidure/ttf2woff2.git"
  },
  "keywords": [
    "ttf",
    "woof2",
    "fonts"
  ],
  "author": {
    "name": "Nicolas Froidure"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nfroidure/ttf2woff2/issues"
  },
  "homepage": "https://github.com/nfroidure/ttf2woff2",
  "dependencies": {
    "bufferstreams": "^1.0.2",
    "bindings": "^1.2.1",
    "nan": "^1.8.4",
    "node-gyp": "^2.0.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.16",
    "miniquery": "^1.1.1",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "^0.0.2"
  },
  "preferGlobal": "true",
  "bin": {
    "ttf2woff2": "bin/ttf2woff2.js"
  },
  "gypfile": true,
  "gitHead": "6c778fada956093d18ce20b5e61d69808239f05c",
  "_id": "ttf2woff2@1.2.3",
  "_shasum": "f455406de18866cd8a3131e35b7665f322416189",
  "_from": "ttf2woff2@>=1.2.3 <2.0.0",
  "_npmVersion": "2.14.2",
  "_nodeVersion": "4.0.0",
  "_npmUser": {
    "name": "nfroidure",
    "email": "nfroidure@elitwork.com"
  },
  "dist": {
    "shasum": "f455406de18866cd8a3131e35b7665f322416189",
    "tarball": "https://registry.npmjs.org/ttf2woff2/-/ttf2woff2-1.2.3.tgz"
  },
  "maintainers": [
    {
      "name": "nfroidure",
      "email": "nfroidure@elitwork.com"
    }
  ],
  "_resolved": "https://registry.npmjs.org/ttf2woff2/-/ttf2woff2-1.2.3.tgz"
}
